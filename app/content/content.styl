#content
  margin 0 auto
  width 1000px
  border thin solid black

.homes, .baths, .photos
  height 210px
  width 200px
  margin-top 15px
  margin-left 84px
  display inline-block
  cursor pointer
  vertical-align top
  border thin solid black
  &:hover
    border thin solid red
  
body 
  background-color #DDDDDD
  
.main
  height 500px
  width 900px
  margin-top 20px
  margin-left 20px
  border thin solid black
  
.main p, .main h2, .main img
  margin-left 10px


.ui-tabs .ui-tabs-nav li
  margin-left 20px
.ui-tabs .ui-tabs-nav .ui-tabs-anchor
  background-color rgba(116, 178, 226, 0.65)
  padding-right 40px
  padding-left 40px


.homes h3, .baths h3, .works h3
  margin-left 55px
  margin-bottom 0px
  margin-top 0px
 
.homes span, .baths span
  margin-left 55px


// single item when open
.hideLi
  display none
  
.singleImg, .singleGallery
  display inline-block
  vertical-align top
  
.singleGallery
  height 460px
  width 160px
  border thin solid black
  
.singleGallery .image, .singleGallery .planeFirst, .singleGallery .planeSecond
  height 150px
  width 150px
  border thin solid black
  

// gallery
.works
  height 200px
  width 200px
  margin-top 15px
  margin-left 84px
  display inline-block
  cursor pointer
  vertical-align top
  border thin solid black
  &:hover
    border thin solid red


.boxShadow
  position fixed
  opacity .8
  background-color rgb(51, 51, 51)
  height 100%
  width 100%
  top 0
  left 0
  z-index 100
  display none

.box
  position fixed
  left 0
  right 0
  top 0
  bottom 0
  z-index 110
  display none
  
.contentGallery
  position absolute
  margin 30px 70px 102px
  z-index 150
  border thin solid red
  top 0
  left 0
  
.contentImg
  position relative
  max-width 100%
  max-height 100%
  background-color white
  border thin solid green
  z-index 200
  
.contentSpacer
  height 100px
  position absolute
  top 100%
  left 0
  width 1px
  
.listGallery
  position fixed
  bottom 0
  left 0
  height 100px
  border thin solid red
  z-index 110
  display none
  opacity .8
  background-color rgb(51, 51, 51)
  width 100%
  margin 0 auto
  
.mini
  height 90px
  width 90px
  border thin solid blue
  display inline-block
  
.premini
  height 100%
  width 400px
  margin 0 auto
  border thin solid green