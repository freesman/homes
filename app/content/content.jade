hr.line
.hidetabs
#tabs
  ul
    li(id='firstTab')
      a(href="#tabs-main", id='tab-main') Главная
    li
      a(href="#tabs-homes", id='tab-home') Дома
    li
      a(href="#tabs-baths", id='tab-bath') Бани
    li
      a(href="#tabs-gallery", id='tab-gallery') Портфолио
    li
      a(href="#tabs-another" ,id='tab-another') Другое
    li.hideLi
      a(href="#tabs-single") 3D

  #tabs-main
    .main
      h2 Мы занимаемся строительством домов из профилированного бруса.
      ul
        li проектирование будущего сооружения
        li установка фундамента
        li возведение силовой части здания
        li кровельные работы
        li отделка
      img(src='content/img/home.jpg' height='350px' width='470px')
  #tabs-homes(data-bind = 'collection:$collection')
  #tabs-baths(data-bind = 'collection:$collection')
  #tabs-gallery
    .gallery-name Галлерея баньки
    .gallery-bath-collection(data-bind = 'collection:$collection')
    .gallery-name Галлерея домика
    .gallery-home-collection(data-bind = 'collection:$collection')
  #tabs-another
    ul
      li заборы
      li двери
      li окна
      li отделка
  #tabs-single
    .project
    img.singleImg(src='content/img/home-3d.png' height='460px' width='500px')
    .singleGallery(data-id = '')
      img.image(src='content/img/home-3d.png' height='150px' width='150px')
      img.planeFirst(src='content/img/home-1p.png' height='150px' width='150px')
      img.planeSecond(src='content/img/home-2p.png' height='150px' width='150px')
    .butClose
    .price цена 500 $
    .description 5кв бревно 150 бла бла